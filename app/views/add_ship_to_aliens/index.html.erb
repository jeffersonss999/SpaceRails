<p id="notice"><%= notice %></p>

<h1>Add Ship To Aliens</h1>

<table>
  <thead>
    <tr>
      <th>Ship</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @add_ship_to_aliens.each do |add_ship_to_alien| %>
      <tr>
        <td><%= add_ship_to_alien.ship %></td>
        <td><%= link_to 'Show', add_ship_to_alien %></td>
        <td><%= link_to 'Edit', edit_add_ship_to_alien_path(add_ship_to_alien) %></td>
        <td><%= link_to 'Destroy', add_ship_to_alien, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

